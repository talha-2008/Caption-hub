  { "id": "en-motivational-021", "text": "Fall seven times, stand up eight.", "tags": ["motivational"], "length": 42, "mood": "inspired", "lang": "en" },
  { "id": "en-motivational-022", "text": "Hard work beats talent when talent is lazy.", "tags": ["motivational"], "length": 50, "mood": "bold", "lang": "en" },
  { "id": "en-motivational-023", "text": "Be the energy you want to attract.", "tags": ["positive"], "length": 44, "mood": "inspired", "lang": "en" },
