  { "id": "en-motivational-013", "text": "Work in silence, let success make the noise.", "tags": ["motivational"], "length": 50, "mood": "inspired", "lang": "en" },
  { "id": "en-motivational-014", "text": "Focus on your goals, not their opinions.", "tags": ["motivational"], "length": 46, "mood": "bold", "lang": "en" },
  { "id": "en-motivational-015", "text": "Be stronger than your excuses.", "tags": ["short"], "length": 39, "mood": "inspired", "lang": "en" },
